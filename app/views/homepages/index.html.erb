            <%= image_tag "nologo.png"%>
            <% else %>
            <%= image_tag business.logo_url%>
            <% end %>
            <div class="caption">
              <h4><%= link_to business.name, business_path(business) %></h4>
              <h6><%= business.description %></h6>
            </div>
          </div>
        </div>
        <% end %>
        <div class="col-xs-12 text-center"><a href="#" class="h6">View All ></a></div>
      </div>
    </div>
